FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1e-6;
        relTol          0.1;
    }

    U
    {
	solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1e-6;
        relTol          0.1;
    }
}

SIMPLE
{
    residualControl
    {
        p               1e-5;
        U               1e-5;
    }
    nNonOrthogonalCorrectors 0;
    consistent               yes;
    momentumPredictor        off;
    pRefCell                 0;
    pRefValue                0;

}

relaxationFactors
{
    fields
    {
        p               0.3;
	U               0.3;
    }
    equations
    {
        U               0.9;
    }
}

